{
  "name": "vue-simple-admin-starter",
  "version": "0.0.1",
  "description": "A basic backend management system template based on Vue3 and pro-components",
  "author": "puppetk-666<https://github.com/puppet-666>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:puppet-666/vue-simple-admin-starter.git"
  },
  "keywords": [
    "vue3",
    "ant-design-vue",
    "ant-design-pro",
    "admin",
    "template-project"
  ],
  "scripts": {
    "dev": "vite --config ./config/vite.config.dev.ts",
    "build": "vue-tsc --noEmit && vite build --config ./config/vite.config.prod.ts",
    "preview": "npm run build && vite preview --host",
    "typecheck": "vue-tsc --noEmit && vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@ant-design-vue/pro-layout": "^3.2.4",
    "@ant-design/icons-vue": "^6.1.0",
    "@formily/core": "^2.2.29",
    "@formily/vue": "^2.2.29",
    "@vueuse/core": "^10.1.2",
    "@vueuse/integrations": "^10.1.2",
    "ant-design-vue": "4.x",
    "axios": "^1.4.0",
    "dayjs": "^1.11.9",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.3",
    "vue": "^3.2.35",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@chuhoman/eslint-config": "^0.1.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/lodash-es": "^4.17.7",
    "@types/mockjs": "^1.0.7",
    "@types/node": "^17.0.43",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-vue": "^4.2.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "eslint": "^8.40.0",
    "less": "^4.1.3",
    "mockjs": "^1.1.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "typescript": "~4.5.5",
    "unplugin-auto-import": "^0.15.0",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.3.5",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vue-tsc": "^1.0.24"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
